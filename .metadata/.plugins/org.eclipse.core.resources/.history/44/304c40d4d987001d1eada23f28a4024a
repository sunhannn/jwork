
public class Apple {

	public static void main(String[] args) {
	int max =3;	
	for(int i =2;i <max; i = i+1) {
		System.out.println(i+"ë‹¨");
		
       for(int j=1; j<10; j= j+1) {
			int result = i * j;
			System.out.println(i+"x"+j+"="+result);
			if(!(i == max && j ==9))System.out.println();
		}
			if(i<max) 
				System.out.println();	
			
       
	   }
	
	
    }
}