package conex;

class Fruits{
	  String fname;
	  int fpirce;
	  String fspace;
	  public void fmd(String fname, int fpirce, String fspace) {
		  this.fname=fname;
		  this.fpirce=fpirce;
		  this.fspace=fspace;
	  }
	  
}

public class FruitEx {
  public static void main(String[] args) {
	  
	  Fruits fe = new Fruits();
	  fe.fmd("사과", 20000, "청송");
	  
  }
}
