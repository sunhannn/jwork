package com.study;

public class Practice003 {
	/*
	 * 	while 문
	 * while (조건식){ //결과값이 true나 false로 반환 함
	 * 실행문;
	 * 
	 * 제어할 수 있는 실행문 또는 제어어가 존재해야 함
	 * 
	 */
	public static void main(String[] args) {
		
		System.out.println("while문 1");
		
		int total =0, i = 1;
		
		while (i <= 100) {
			total += i;
			i++;
		}
		System.out.println(total);
		
	}

}
