package com.study;

public class FruitsEx {

	public static void main(String[] args) {
		
		Fruits f = new Fruits();
		f.fmd("사과", 2000, "청송");
		
	}

}

class Fruits {
	//과일 이름, 가격, 원산지
	String name;
	int price;
	String space;
	
	public void fmd(String name, int price, String space) {
		this.name = name;
		this.price = price;
		this.space = space;
	}
	public String getName(String name) {
		return name;
	}
	
}
