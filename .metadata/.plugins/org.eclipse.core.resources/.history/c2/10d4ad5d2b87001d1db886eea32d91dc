package com.study;

public class Practice002 {

	public static void main(String[] args) {
		// 3번 반복하기
		for (int i = 1 ; i <= 3; i++) {
			System.out.print("가");
		}
		System.out.println();
		for (int i = 2; i < 5; i++) {
			System.out.print("가");
		}
		System.out.println();
		for (int i =3 ; i > 0; i--) {
			System.out.print("가");
		}
		
		
	}

}
