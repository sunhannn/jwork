package com.study;

public class Practice007 {

	public static void main(String[] args) {
		
		int i, y, t =0;
		
		for (i = 1 ; i  <= 9; i++) {
			System.out.println(i + "단입니다.");
			for (y=1; y <= 9; y++) {
				t = i * y;
				System.out.println(i + " X " + y + " = " + t);
			}
		}
		
		
		
		

	}

}
