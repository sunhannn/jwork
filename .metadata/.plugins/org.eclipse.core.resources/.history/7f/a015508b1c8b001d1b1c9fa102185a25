package conex;

public class CarEx {
	public static void main(String[] args) {
		Car a = new Car();
		a.cmd("그랜져", 2022, 10000);
	}
}


class Car{
	String cname;
	int year;
	int distance;
	
	public void cmd(String cname, int year, int distance) {
		this.cname=cname;
		this.year=year;
		this.distance=distance;
	}
	
}
