package collectionex;
import java.util.Scanner;
public class TestEx {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);//스캐너 사용이 가능합니다
		System.out.println("1: ");
		String s1 = sc.next();//한 단어단위를 입력을 받겠습니다 공백이 없어요
		int i = sc.nextInt();//숫자를 입력하세요
		s1 = sc.next();//문자를 입력하세요
		System.out.println("2: ");
		s1 = sc.next();
		
	}

}
