package conex;

class Fruits{
	  String fname;
	  int fprice;
	  String fspace;
	  public void fmd(String fname, int fpirce, String fspace) {
		  this.fname=fname;
		  this.fprice=fprice;
		  this.fspace=fspace;
	  }
	 String getName() { return fname; }
     int getPrice() { return fprice; }
     String getSpace() { return fspace; }
	 
}

public class FruitEx {
  public static void main(String[] args) {
	  
	  Fruits fe = new Fruits();
	  fe.fmd("사과", 20000, "청송");
	  
  }
}
