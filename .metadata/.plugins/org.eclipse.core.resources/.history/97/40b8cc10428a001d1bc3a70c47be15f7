package methodex;

public class Ex01 {
   
	static int sum(int x, int y) { 
		int c; 
		c = x + y; 
		return c; //or return c = x + y;
				
	}
	
	public static void main(String[] args) { 
		int a = 10, b = 20;
		
		//sum(a, b); -> sum(10, 20);
		//sum(4, 5); -> sum(4,5);
		
		System.out.println("x + y =" + sum(a, b));
		System.out.println("x + y =" + sum(4, 5));
		
	}
}
