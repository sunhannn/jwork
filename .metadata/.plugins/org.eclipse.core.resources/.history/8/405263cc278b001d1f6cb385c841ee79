package com.study;

public class CarEx {

	public static void main(String[] args) {

		Car c1 = new Car();

		c1.setSyu("소렌토");
		c1.setNen(2020);
		c1.setKyo(20000);
		c1.print();

	}

}

class Car {

	String syu;
	int nen, kyo;

	public void setSyu(String syu) {
		this.syu = syu;
	}

	public void setNen(int nen) {
		this.nen = nen;
	}

	public void setKyo(int kyo) {
		this.kyo = kyo;
	}

	public String getSyu() {
		return syu;
	}

	public int getNen() {
		return nen;
	}

	public int getkyo() {
		return kyo;
	}

	public void print() {
		System.out.printf("차종은 : %s이고, %d년식이고, 총 %,dKm를 탔습니다.", syu, nen, kyo);

	}

}
