
public class Daa {

	public static void main(String[] args) {
		// i = i+1
				for (int i = 1; i <= 3; i++) { // i=4 /
					System.out.println("가"); //가가가 
				}
		           System.out.println();
		           
		           //i=i+1 
		           for(int i =2; i< 5; i++) {// i=5
		        	   System.out.println("가"); //가가가
		           }
		           System.out.println();
		           
		           //i=i-1
		           for(int i= 3; i>0 ; i--) { //i=0
		        	   System.out.println("가");//가가가

	}

}
