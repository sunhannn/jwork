package conex;

public class FoodEx {
  public static void main(String[] args) {
	  // 김치 1년 20000원
      Food a = new Food();
      a.fmd("김치", 1, 20000);
      System.out.println(a.fname + ", " + a.fyear + ", "+ a.fprice);
  }
}


class Food {
	String fname;
	int fyear;
	int fprice;
	void fmd(String fname, int fyear, int fprice){
		this.fname = fname;
		this.fyear = fyear;
		this.fprice = fprice;
		
	}
}