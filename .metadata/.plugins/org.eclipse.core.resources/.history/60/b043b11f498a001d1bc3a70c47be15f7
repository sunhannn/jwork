package methodex;

public class Ex03 {
    
    public static void main(String[] args) {
    	int result = add(3, 5);
    	System.out.println(result);
    	
    	int[] result2 = {0};
    	add(3, 5, result2);
    	System.out.println(result2[0]);
    	}
    
    static int add(int a, int b) {return a +b;}
    
    static void add(int a, int b, int[] result){result[0] =a +b;}
    	
}
