package methodex;

public class Ex03 {
    
    public static void main(String[] args) { //메인메소드 선언
    	 int score = 72;

    	 switch( score / 10 ) {
    	     case 10 : case 9 : System.out.println("A등급“);
    	     case 8 : System.out.println("B등급“);
    	     case 7 : System.out.println("C등급“);
    	     case 6 : System.out.println("D등급“);
    	     default : System.out.println("F등급“);
    	 }
}
