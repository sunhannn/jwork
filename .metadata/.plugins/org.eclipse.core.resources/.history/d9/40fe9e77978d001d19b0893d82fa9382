package com.inheritance;

public class CastingTest01 {

	public static void main(String[] args) {
		
		
		
		
		
	}

}

class Person {
	String name;
	public Person(String name) {
		this.name = name;
	}
	
}
class Student extends Person {
	String dept;
	public Student(String name) {
		super(name);
	}
}